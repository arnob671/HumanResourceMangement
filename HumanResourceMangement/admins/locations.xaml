        <Frame x:Name="frame1" Content="" HorizontalAlignment="Left" Height="185" Margin="15,176,0,0" VerticalAlignment="Top" Width="584" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
        <Label x:Name="label5" Content="" HorizontalAlignment="Left" Margin="18,178,0,0" VerticalAlignment="Top" Width="581" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}"/>
        <Button x:Name="button2" Content="add" HorizontalAlignment="Left" Margin="23,142,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>
        <Button x:Name="button3" Content="delete" HorizontalAlignment="Left" Margin="114,143,0,0" VerticalAlignment="Top" Width="75" Click="button3_Click_1"/>
